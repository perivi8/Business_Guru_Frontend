{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/tmis-business-guru"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://business-guru-backend.onrender.com/api/$1"
    },
    {
      "src": "/test.html",
      "dest": "/test.html"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpe?g|gif|svg|woff2?|ttf|eot|ico))$",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}